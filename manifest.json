{
    "manifest_version": 2,
    "name": "Dribbble Night",
    "description": "Dark Mode for Dribbble.",
    "version": "0.11",
    "content_scripts": [
        {
            "matches": [
                "http://*.dribbble.com/*",
                "https://*.dribbble.com/*"
            ],
            "css": [
                "style.css"
            ],
            "run_at": "document_start"
        }
    ]
}