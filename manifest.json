{
  "manifest_version": 2,
  "name": "Dribbble Night",
  "description": "Dark Mode for Dribbble.",
  "version": "1.0",
  "permissions": ["tabs", "https://*.dribbble.com/*", "storage"],
  "content_scripts": [
    {
      "matches": ["http://*.dribbble.com/*", "https://*.dribbble.com/*"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ],
  "browser_action": {
    "default_title": "Dark Mode for Dribbble."
  },
  "web_accessible_resources": ["style.css", "userProfile.css", "content.js"],
  "homepage_url": "https://www.dribbbbble.com"
}
